5
(
    lowerWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          4;
        startFace       52;
    }
    upperWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          4;
        startFace       56;
    }
    front
    {
        type            cyclic;
        inGroups        List<word> 1(cyclic);
        nFaces          8;
        startFace       60;
